<%= provide(:title, "project #{@project.name}") %>

<section class="project">
  <header class="project-header">
    <img alt="" src="#" class="project-img" />
    <dt><h1 class="project-name"><%= @project.name %></h1></dt>
    <p class="project-details">
      <span class="creator">Created By: <%= print_member(@project.creator_id) %></span>
      <!-- <a class="project-category" href="#"></a> -->
      <br /><span class="upd_at">Last Updated: <%= @project.updated_at %></span>
    </p>
  </header>
  <dd class="project-info">
    <%= @project.description %>
  </dd>
</section>

<%= render "links" %>
